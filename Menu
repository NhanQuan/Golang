package main

import (
	"fmt"
    "os"
)

func main() {
    user := os.Args[1]
    password := os.Args[2]

    user_check := "A"
    password_check := "123456"
    
    if user == user_check {
        if password == password_check {
            fmt.Println("Access granted to", user)
        } else {
            fmt.Println("Invalid password for ", user) 
        }          
    } else {
        fmt.Println("Access denied for", user)
    }
}
